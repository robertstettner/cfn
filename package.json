{
  "name": "cfn",
  "version": "0.18.0",
  "description": "Simple AWS CloudFormation for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nordstrom/cfn.git"
  },
  "files": [
    "index.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "xo": {
    "space": 4,
    "rules": {
      "max-lines": [
        "warn",
        400
      ],
      "babel/object-curly-spacing": [
        "error",
        "always"
      ],
      "one-var": "off",
      "operator-linebreak": [
        "error",
        "before"
      ],
      "no-use-extend-native/no-use-extend-native": "off"
    }
  },
  "keywords": [
    "Cloud",
    "Formation",
    "CloudFormation",
    "Cloud-Formation",
    "Node.js",
    "Stack",
    "Deploy"
  ],
  "author": "Andy Day",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nordstrom/cfn/issues"
  },
  "contributors": [
    "Andy Day (https://github.com/andyday)",
    "Katie Steinberg (https://github.com/steinbergkh)",
    "Yun Wang <jenny.wang240@gmail.com> (https://github.com/yunwang240)",
    "James Allen (https://github.com/jamesallen-vol)",
    "Brendan Abbott <brendan@bloodbone.ws> (https://github.com/brendo)",
    "Robert Stettner <robert.stettner@gmail.com> (https://github.com/robertstettner)"
  ],
  "homepage": "https://github.com/Nordstrom/cfn#readme",
  "dependencies": {
    "aws-sdk": "^2.7.17",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "https-proxy-agent": "^1.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "sprintf": "^0.1.5",
    "yamljs": "0.2.10"
  },
  "devDependencies": {
    "aws-sdk-mock": "^1.6.1",
    "mocha": "^3.2.0",
    "node-uuid": "^1.4.7",
    "should": "^11.1.2",
    "should-sinon": "0.0.5",
    "sinon": "^1.17.7",
    "xo": "^0.16.0"
  }
}
